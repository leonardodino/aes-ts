{
  "name": "aes-ts",
  "version": "0.0.0-alpha.0",
  "description": "0 dependency, pure TypesScript implementation of the AES block cipher and common modes of operation.",
  "author": "Leonardo Dino",
  "license": "MIT",
  "homepage": "http://github.com/leonardodino/aes-ts",
  "bugs": "http://github.com/leonardodino/aes-ts/issues",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "prettier -l .",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --outDir dist/esm",
    "prebuild": "rm -r dist",
    "build": "yarn -s build:cjs && yarn -s build:esm",
    "pretest": "yarn -s build",
    "test": "node -r esm ./node_modules/.bin/nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardodino/aes-ts.git"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "nodeunit": "^0.11.3",
    "prettier": "2.1.2",
    "typescript": "^4.0.3"
  },
  "keywords": [
    "typescript",
    "crypto",
    "aes",
    "aes-ctr",
    "aes-ofb",
    "aes-ecb",
    "aes-cbc",
    "aes-cfb",
    "encrypt",
    "decrypt",
    "block",
    "cipher"
  ]
}
