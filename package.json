{
  "name": "aes-js",
  "version": "3.1.2",
  "bugs": {
    "url": "http://github.com/ricmoo/aes-js/issues",
    "email": "github@ricmoo.com"
  },
  "description": "A pure JavaScript implementation of the AES block cipher and all common modes of operation.",
  "devDependencies": {
    "esm": "^3.2.25",
    "nodeunit": "^0.11.3",
    "prettier": "2.1.2",
    "typescript": "^4.0.3"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "prettier -l .",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:esm": "tsc --outDir dist/esm",
    "build": "yarn -s build:cjs && yarn -s build:esm",
    "pretest": "yarn -s build",
    "test": "node -r esm ./node_modules/.bin/nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ricmoo/aes-js.git"
  },
  "keywords": [
    "aes",
    "aes-ctr",
    "aes-ofb",
    "aes-ecb",
    "aes-cbc",
    "aes-cfb",
    "encrypt",
    "decrypt",
    "block",
    "cipher"
  ],
  "author": "Richard Moore <me@ricmoo.com>",
  "license": "MIT"
}
